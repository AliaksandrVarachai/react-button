{
  "presets": [
    [
      "@babel/preset-env", {
        "modules": false,
        "targets": {
          "browsers": "> 0.5%, last 2 versions, ie >= 11, not dead"
        }
      }
    ], [
      "@babel/preset-react", {
        "throwIfNamespace": false
      }
    ],
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ]
}
